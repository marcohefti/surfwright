{
  "schemaVersion": 1,
  "missionId": "001-first-pass-orientation",
  "startUrl": "https://the-internet.herokuapp.com/",
  "collectFields": [
    "featureCount",
    "firstFeature"
  ],
  "successCheck": "featureCount >= 40 and firstFeature == \"A/B Testing\"",
  "rules": [
    {
      "field": "featureCount",
      "op": "gte",
      "value": 40
    },
    {
      "field": "firstFeature",
      "op": "eq",
      "value": "A/B Testing"
    }
  ],
  "examplePayload": {
    "featureCount": 45,
    "firstFeature": "A/B Testing"
  }
}
