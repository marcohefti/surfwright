{
  "schemaVersion": 1,
  "missionId": "003-redirect-evidence",
  "startUrl": "https://github.com/marcohefti",
  "collectFields": [
    "profileHomepage",
    "blogUrl"
  ],
  "successCheck": "profileHomepage is non-empty and blogUrl == \"https://blog.heftiweb.ch\"",
  "rules": [
    {
      "field": "profileHomepage",
      "op": "non_empty"
    },
    {
      "field": "blogUrl",
      "op": "eq",
      "value": "https://blog.heftiweb.ch"
    }
  ],
  "examplePayload": {
    "profileHomepage": "https://heftiweb.ch",
    "blogUrl": "https://blog.heftiweb.ch"
  }
}
