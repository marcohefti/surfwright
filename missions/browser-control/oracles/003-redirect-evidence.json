{
  "schemaVersion": 1,
  "missionId": "003-redirect-evidence",
  "startUrl": "http://example.com",
  "collectFields": [
    "requestedUrl",
    "finalUrl",
    "wasRedirected"
  ],
  "successCheck": "requestedUrl starts with http://example.com and finalUrl == \"https://example.com/\" and wasRedirected == true",
  "rules": [
    {
      "field": "requestedUrl",
      "op": "starts_with",
      "value": "http://example.com"
    },
    {
      "field": "finalUrl",
      "op": "eq",
      "value": "https://example.com/"
    },
    {
      "field": "wasRedirected",
      "op": "eq",
      "value": true
    }
  ],
  "examplePayload": {
    "requestedUrl": "http://example.com",
    "finalUrl": "https://example.com/",
    "wasRedirected": true
  }
}
