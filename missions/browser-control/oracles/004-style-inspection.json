{
  "schemaVersion": 1,
  "missionId": "004-style-inspection",
  "startUrl": "https://example.com/",
  "collectFields": [
    "networkRequestCount",
    "networkTopHost",
    "traceInsightName"
  ],
  "successCheck": "networkRequestCount >= 1 and networkTopHost == \"example.com\" and traceInsightName == \"top-host\"",
  "rules": [
    {
      "field": "networkRequestCount",
      "op": "gte",
      "value": 1
    },
    {
      "field": "networkTopHost",
      "op": "eq",
      "value": "example.com"
    },
    {
      "field": "traceInsightName",
      "op": "eq",
      "value": "top-host"
    }
  ],
  "examplePayload": {
    "networkRequestCount": 1,
    "networkTopHost": "example.com",
    "traceInsightName": "top-host"
  }
}
