{
  "schemaVersion": 1,
  "missionId": "005-iframe-edit",
  "startUrl": "https://the-internet.herokuapp.com/iframe",
  "collectFields": [
    "writeText",
    "readBackText",
    "writeMatches"
  ],
  "successCheck": "writeText is non-empty and readBackText is non-empty and writeMatches == true",
  "rules": [
    {
      "field": "writeText",
      "op": "non_empty"
    },
    {
      "field": "readBackText",
      "op": "non_empty"
    },
    {
      "field": "writeMatches",
      "op": "eq",
      "value": true
    }
  ],
  "examplePayload": {
    "writeText": "probe string",
    "readBackText": "probe string",
    "writeMatches": true
  }
}
