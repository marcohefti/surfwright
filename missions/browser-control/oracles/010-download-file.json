{
  "schemaVersion": 1,
  "missionId": "010-download-file",
  "startUrl": "https://the-internet.herokuapp.com/download",
  "collectFields": [
    "downloadStarted",
    "downloadStatus",
    "downloadFinalUrl",
    "downloadFileName"
  ],
  "successCheck": "downloadStarted == true and downloadStatus is non-empty and downloadFinalUrl starts with https://the-internet.herokuapp.com/download/ and downloadFileName is non-empty",
  "rules": [
    {
      "field": "downloadStarted",
      "op": "eq",
      "value": true
    },
    {
      "field": "downloadStatus",
      "op": "non_empty"
    },
    {
      "field": "downloadFinalUrl",
      "op": "starts_with",
      "value": "https://the-internet.herokuapp.com/download/"
    },
    {
      "field": "downloadFileName",
      "op": "non_empty"
    }
  ],
  "examplePayload": {
    "downloadStarted": true,
    "downloadStatus": "completed",
    "downloadFinalUrl": "https://the-internet.herokuapp.com/download/some-file.txt",
    "downloadFileName": "some-file.txt"
  }
}
