{
  "schemaVersion": 1,
  "missionId": "014-table-sort-lastname",
  "startUrl": "https://the-internet.herokuapp.com/tables",
  "collectFields": [
    "rowCount",
    "firstLast",
    "lastLast"
  ],
  "successCheck": "rowCount == 4 and firstLast == \"Bach\" and lastLast == \"Smith\"",
  "rules": [
    {
      "field": "rowCount",
      "op": "eq",
      "value": 4
    },
    {
      "field": "firstLast",
      "op": "eq",
      "value": "Bach"
    },
    {
      "field": "lastLast",
      "op": "eq",
      "value": "Smith"
    }
  ],
  "examplePayload": {
    "rowCount": 4,
    "firstLast": "Bach",
    "lastLast": "Smith"
  }
}
