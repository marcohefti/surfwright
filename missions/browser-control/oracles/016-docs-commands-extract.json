{
  "schemaVersion": 1,
  "missionId": "016-docs-commands-extract",
  "startUrl": "https://docs.astral.sh/uv/getting-started/installation/",
  "collectFields": [
    "installCommand",
    "hasPipeSh"
  ],
  "successCheck": "installCommand contains \"astral.sh/uv/install.sh\" and hasPipeSh == true",
  "rules": [
    {
      "field": "installCommand",
      "op": "contains",
      "value": "astral.sh/uv/install.sh"
    },
    {
      "field": "hasPipeSh",
      "op": "eq",
      "value": true
    }
  ],
  "examplePayload": {
    "installCommand": "$ curl -LsSf https://astral.sh/uv/install.sh | sh",
    "hasPipeSh": true
  }
}
