{
  "schemaVersion": 1,
  "missionId": "016-dynamic-controls-enable",
  "startUrl": "https://the-internet.herokuapp.com/dynamic_controls",
  "collectFields": [
    "enabled",
    "message"
  ],
  "successCheck": "enabled == true and message == \"It's enabled!\"",
  "rules": [
    {
      "field": "enabled",
      "op": "eq",
      "value": true
    },
    {
      "field": "message",
      "op": "eq",
      "value": "It's enabled!"
    }
  ],
  "examplePayload": {
    "enabled": true,
    "message": "It's enabled!"
  }
}
